{
  "editor.formatOnSave": true,
  "cSpell.words": [
    "buildx",
    "dotenv",
    "dotenvy",
    "gcloud",
    "hookform",
    "tailwindcss",
    "tanstack",
    "trivago",
    "wiremock"
  ],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.diagnostics.experimental.enable": false,
  "deepscan.ignoreConfirmWarning": true
}
